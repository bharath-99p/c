#include<iostream>
using namespace std;
 int main(){
    int arr[5],temp;
    for(int i=0;i<5;i++){
        cout<<"enter :";
        cin>>arr[i];
    }
    for(int i=1;i<=5;i++){
        for(int j=i-1;j>=0 && arr[j]>arr[j+1];j--){
            temp=arr[j];
            arr[j]=arr[j+1];
            arr[j+1]=temp;
        }
    }
    cout<<"\n sorted  :";
    for(int i=0;i<5;i++){
        cout<<arr[i]<<"\t";
    }
 }
